generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "sqlite"
  url      = "file:./dev.db"
}

model Song {
  id         Int      @id @default(autoincrement())
  title      String
  artist     String
  thumbnail  String
  url        String
  duration   Int
  created_at DateTime @default(now())
  updated_at DateTime @updatedAt
}
